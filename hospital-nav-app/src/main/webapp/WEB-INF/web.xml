<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<context-param>
		<param-name>cparam1</param-name>
		<param-value>10</param-value>
	</context-param>

	<context-param>
		<param-name>cparam2</param-name>
		<param-value>20</param-value>
	</context-param>

	<servlet>
		<servlet-name>OpdSeervlet</servlet-name>
		<display-name>OpdSeervlet</display-name>
		<description></description>
		<servlet-class>com.java.servlets.OpdSeervlet</servlet-class>

		<init-param>
			<param-name>iparam1</param-name>
			<param-value>20</param-value>
		</init-param>

		<init-param>
			<param-name>iparam2</param-name>
			<param-value>30</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>DeptForwardServlet</servlet-name>
		<display-name>DeptForwardServlet</display-name>
		<description></description>
		<servlet-class>com.java.servlets.DeptForwardServlet</servlet-class>
		<init-param>
			<param-name>iparam1</param-name>
			<param-value>25</param-value>
		</init-param>
		<init-param>
			<param-name>iparam2</param-name>
			<param-value>35</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>OpdSeervlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeptForwardServlet</servlet-name>
		<url-pattern>/dept-forward</url-pattern>
	</servlet-mapping>
</web-app>
